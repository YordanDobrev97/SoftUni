<ul>
    {{#each cats}}
        <li>
            <img src="{{path}}" width="250" height="250" alt="Card image cap">
            <div class="info">
                <button class="showBtn">Show status code</button>
                <div class="status" style="display: none" id="{{id}}">
                    <h4>{{statusCode}}</h4>
                    <p>{{message}}</p>
                </div>
            </div>
        </li>
    {{/each}}
</ul>